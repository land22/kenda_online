<!-- Library for adding dinamically elements -->
<script src="{{ asset('AdminAssets/js/arrive.min.js')}}" type="text/javascript"></script>

<!-- Forms Validations Plugin -->
<script src="{{ asset('AdminAssets/js/jquery.validate.min.js')}}"></script>

